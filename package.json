{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode=development",
    "save": "git add . && git commit -m save && git push",
    "build": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ColtHands/bestofblockchain.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ColtHands/bestofblockchain/issues"
  },
  "homepage": "https://github.com/ColtHands/bestofblockchain#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/transform-runtime",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-nullish-coalescing-operator",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-transform-shorthand-properties"
    ]
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "commonjs": true,
      "jquery": true
    },
    "globals": {
      "React": "readonly",
      "vis": "readonly"
    },
    "rules": {
      "no-prototype-builtins": [
        "off",
        "never"
      ],
      "semi": [
        "warn",
        "never"
      ],
      "quotes": [
        "warn",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "indent": [
        "warn",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "prefer-const": [
        "warn",
        {
          "destructuring": "any"
        }
      ],
      "brace-style": [
        "warn",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "object-curly-spacing": [
        "warn",
        "always",
        {
          "objectsInObjects": false
        }
      ],
      "key-spacing": [
        "warn"
      ],
      "switch-colon-spacing": [
        "warn"
      ],
      "space-before-blocks": [
        "warn"
      ],
      "space-before-function-paren": [
        "warn",
        "never"
      ],
      "comma-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "arrow-spacing": [
        "warn"
      ],
      "no-unreachable": "warn",
      "no-useless-escape": "warn",
      "no-unused-vars": "warn",
      "array-bracket-spacing": [
        "warn",
        "never"
      ],
      "keyword-spacing": [
        "warn",
        {
          "overrides": {
            "if": {
              "after": false
            },
            "for": {
              "after": false
            },
            "switch": {
              "after": false
            },
            "while": {
              "after": false
            }
          }
        }
      ],
      "comma-dangle": [
        "warn",
        {
          "arrays": "never",
          "objects": "never",
          "imports": "never",
          "exports": "never",
          "functions": "never"
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/plugin-transform-shorthand-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/react": "^18.0.6",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "prop-types": "^15.8.1",
    "yargs": "^17.4.0"
  }
}
